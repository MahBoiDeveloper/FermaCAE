procedure Tplast_fd_form.Del_Cut(xory:char ; num:integer);
 //удаление разбивки
var
  i,j,nn,ii,iii,jj,jjj:integer;
  cz:array[1..plast_max_zak,1..2] of extended;//координаты закреплений
  cf:array[1..plast_max_for,1..2] of extended;//координаты нагрузок
   currentnx,currentny : integer;
begin
     CurrentNx:=0;
     CurrentNy:=0;
     if xory='X' then CurrentNx:=num;
     if xory='Y' then CurrentNy:=num;
     Main_Form.StatusBar1.Panels[1].Text := '';
     Main_Form.StatusBar1.Panels[2].Text := '';

        if currentnx<>0 then
        begin
        //проверка закреплений

         for i:=1 to p.kz1 do
         begin
          if   (p.kx1>=p.ky1) and (p.zak1[i]>=(currentnx-1)*p.ky1+1) and (p.zak1[i]<= (currentnx)*p.ky1)then
            begin
              Main_Form.StatusBar1.Panels[1].Text := 'На линии разбиения присутствует закрепление.';
              Main_Form.StatusBar1.Panels[2].Text := 'Выберите другую линию.';
              beep;
              Exit;
            end;
            for j:=1 to p.ky1 do
            if   (p.kx1<p.ky1) and (p.zak1[i]=(p.kx1)*(j-1)+currentnx) then
            begin
              Main_Form.StatusBar1.Panels[1].Text := 'На линии разбиения присутствует закрепление.';
              Main_Form.StatusBar1.Panels[2].Text := 'Выберите другую линию.';
              beep;
              Exit;
