   for i:=1 to Plast_M.TPlast_Form(Main_Form.ActiveMDIChild).Plast.kx1 do
     Plast_M.TPlast_Form(Main_Form.ActiveMDIChild).Plast.xm1[i]:=Plast_M.TPlast_Form(Main_Form.ActiveMDIChild).Plast.xm1[i]*l_factor;
